{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/app.js"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo test",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--features=major_libs",
                    "--test=major_libs"
                ]
            },
            "program": "${cargo:program}",
            "args": ["--nocapture", "react_dom"],
            "sourceLanguages": [
                "rust"
            ],
            "env": {
                // "RUST_LOG": "resp:debug"
            },
        },
        {

            "type": "lldb",
            "request": "launch",
            "name": "Simple Example",
            "cargo": {
                "args": [
                    "build",
                    "--example",
                    "simple"
                ]
            },
            "program": "${cargo:program}"
        }
    ]
}